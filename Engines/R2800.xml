<?xml version="1.0"?>

<!--
  File:     R2800.xml
  Author:   AeromatiC++ v 3.3.0

  See: http://wiki.flightgear.org/JSBSim_Engines#FGPiston

  Inputs:
    name:           R2800
    type:           Piston Engine
    power:          2600 hp
-->

<piston_engine name="R2800">
  <minmp unit="INHG">         10.0 </minmp>
  <maxmp unit="INHG">         28.5 </maxmp>
    <displacement unit="IN3"> 2800 </displacement>
  <maxhp>        2600 </maxhp>
  <cycles>         4.0 </cycles>
  <idlerpm>      400.0 </idlerpm>
  <maxrpm>      3000.0 </maxrpm>
  <sparkfaildrop>  0.15 </sparkfaildrop>
  <volumetric-efficiency> 0.85 </volumetric-efficiency>
  <man-press-lag> 0.1 </man-press-lag>
  <static-friction  unit="HP"> 13 </static-friction>
  <starter-torque> 2080 </starter-torque>
  <starter-rpm> 1000 </starter-rpm>
 <!-- Defining <bsfc> over-rides the built-in horsepower calculations -->
  <bsfc>           0.275 </bsfc>
  <stroke unit="IN">  4.375 </stroke>
  <bore unit="IN">    5.125 </bore>
  <cylinders>         18  </cylinders>
  <compression-ratio>  8.0 </compression-ratio>
  <boostauto>      1 </boostauto>
  <numboostspeeds>   2 </numboostspeeds>
  <boostoverride>    0 </boostoverride>
  <takeoffboost>     1</takeoffboost> 


 <!-- Low-speed supercharger; see table above.  The rated boost is on top of
       sea level ambient pressure. -->
  <ratedboost1 unit="INHG"> 8.00 </ratedboost1>
  <ratedpower1> 2600 </ratedpower1>
  <ratedrpm1>   3000 </ratedrpm1>
  <ratedaltitude1> 0 </ratedaltitude1>

  <!-- High-speed supercharger; see table above. -->
  <ratedboost2 unit="INHG"> 14.00 </ratedboost2>
  <ratedpower2> 2800 </ratedpower2>
  <ratedrpm2>   3000 </ratedrpm2>
  <ratedaltitude2> 15000 </ratedaltitude2>


</piston_engine>
